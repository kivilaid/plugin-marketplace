{
  "name": "example-full-featured",
  "version": "1.0.0",
  "description": "A complete example plugin demonstrating all component types: commands, agents, hooks, and MCP servers",
  "author": {
    "name": "ando",
    "email": "ando@kivilaid.ee"
  },
  "homepage": "https://github.com/kivilaid/plugin-marketplace",
  "repository": "https://github.com/kivilaid/plugin-marketplace",
  "license": "MIT",
  "keywords": ["example", "demo", "full-featured", "tutorial"],
  "commands": [
    "./commands/hello.md",
    "./commands/deploy.md"
  ],
  "agents": [
    "./agents/code-reviewer.md",
    "./agents/test-generator.md"
  ],
  "hooks": "./hooks/hooks.json",
  "mcpServers": {
    "example-server": {
      "command": "node",
      "args": ["${CLAUDE_PLUGIN_ROOT}/mcp-servers/example-server.js"],
      "env": {
        "EXAMPLE_VAR": "example-value"
      }
    }
  }
}
